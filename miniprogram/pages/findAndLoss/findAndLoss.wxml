<!--pages/findAndLoss/findAndLoss.wxml-->
  <view class="my">
    <view class="my-item flex-wrp" bindtap="findTap">
      <view class="myitem-icon flex-item" >
        <image src="/asserts/icons/tab_icon_found_n@2x.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>寻物启事</text>
      </view>
    </view>
    <view class="my-item flex-wrp" bindtap="lostTap">
      <view class="myitem-icon flex-item" >
        <image src="/asserts/icons/tab_icon_lost_n@2x.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>失物招领</text>
      </view>
    </view>
    <view class="my-item flex-wrp" bindtap="releaseTap">
      <view class="myitem-icon flex-item" >
        <image src="/asserts/icons/tab_icon_release_n@2x.png"></image>
      </view>
      <view class="myitem-name flex-item">
        <text>发布</text>
      </view>
    </view>
  </view>

